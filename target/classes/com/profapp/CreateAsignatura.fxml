   <children>
      <Label layoutX="113.0" layoutY="20.0" text="Introduzca nombre de asignatura" />
      <TextField fx:id="textField" layoutX="121.0" layoutY="47.0" />
      <HBox layoutX="135.0" layoutY="100.0" spacing="5.0">
         <children>
            <Button fx:id="cancelButton" mnemonicParsing="false" onAction="#onCancelButtonPress" text="Cancelar" />
            <Button fx:id="saveButton" mnemonicParsing="false" onAction="#onSaveButtonPress" text="Guardar" />
         </children>
      </HBox>
   </children>
</AnchorPane>
