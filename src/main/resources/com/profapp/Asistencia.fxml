<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.cell.PropertyValueFactory?>

<TableView fx:id="tableView" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/21" fx:controller="com.profapp.Controller">
   <columns>
      <TableColumn prefWidth="200" text="Nombre">
         <cellValueFactory>
            <PropertyValueFactory property="nombre" />
         </cellValueFactory>
      </TableColumn>
      <TableColumn fx:id="asistenciaColumn" prefWidth="100" text="Asistencia">
         <cellValueFactory>
            <PropertyValueFactory property="asistio" />
         </cellValueFactory>
      </TableColumn>
   </columns>
</TableView>
